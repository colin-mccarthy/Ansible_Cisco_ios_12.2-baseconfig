{% for iface,data in interface.items() %}
interface {{ iface }}
{% if data.description is defined %}
description {{ data.description }}
{% endif %}
{% if data.admin_state is defined %}
admin_state: {{ data.admin_state }}
{% endif %}
{% if data.speed %}
speed {{ data.speed }}
{% endif %}
{% if data.duplex %}
duplex {{ data.duplex }}
{% endif %}
{% if data.vlan %}
switchport access vlan {{ data.vlan }}
{% endif %}
{% endfor %}
